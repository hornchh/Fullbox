<?xml version="1.0" encoding="UTF-8"?>  
<Configuration status="DEBUG">
    <Appenders>  
        <Console name="ConsoleLogger" target="SYSTEM_OUT"> 
            <PatternLayout pattern="%d{HH:mm:ss} [%t] %p %C (%L) - %m%n"/>
        </Console>
    </Appenders> 
    <Loggers>  
        <Root level="trace">  
            <AppenderRef ref="ConsoleLogger"/>  
        </Root>  
    </Loggers>  
</Configuration> 

